@model List<Order>

@{
    ViewBag.Title = "Index";
}

<h2>Listado de Libros</h2>
<hr />

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Ship Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var libro in Model)
        {
            <tr>
                <td>@libro.ShipName</td>
            </tr>
        }
    </tbody>

</table>

